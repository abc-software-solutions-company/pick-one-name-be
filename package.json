{
  "name": "pick-one-name-be",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles",
    "test:clean": "rimraf coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "app:down": "docker-compose stop app",
    "app:log": "docker-compose logs -f app",
    "app:up": "docker-compose up -d app",
    "db:down": "docker-compose stop db",
    "db:log": "docker-compose logs -f db",
    "db:up": "docker-compose up -d db",
    "dbtest:down": "docker-compose stop dbtest",
    "dbtest:log": "docker-compose logs -f dbtest",
    "dbtest:up": "docker-compose up -d dbtest",
    "redis:down": "docker-compose stop redis",
    "redis:log": "docker-compose logs -f redis",
    "redis:up": "docker-compose up -d redis",
    "docker:up": "yarn db:up && yarn dbtest:up && yarn redis:up",
    "docker:log": "yarn db:log && yarn dbtest:log && yarn redis:log",
    "docker:down": "yarn db:down && yarn dbtest:down && yarn redis:down",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "typeorm:ts": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm",
    "typeorm:dropschema": "yarn typeorm:ts schema:drop -d ./src/database/data-source.ts",
    "typeorm-win:dropschema": "yarn typeorm schema:drop -d ./src/database/data-source.ts",
    "migration:generate": "yarn typeorm:ts migration:generate -d ./src/database/data-source.ts",
    "migration-win:generate": "yarn typeorm migration:generate -d ./src/database/data-source.ts",
    "migration:revert": "yarn typeorm:ts migration:revert -d ./src/database/data-source.ts",
    "migration-win:revert": "yarn typeorm migration:revert -d ./src/database/data-source.ts",
    "migration:run": "yarn typeorm:ts migration:run -d ./src/database/data-source.ts",
    "migration-win:run": "yarn typeorm migration:run -d ./src/database/data-source.ts",
    "migration:create": "yarn typeorm:ts migration:create",
    "seed:revert": "yarn typeorm:ts migration:revert -d ./src/database/seed-data-source.ts",
    "seed-win:revert": "yarn typeorm migration:revert -d ./src/database/seed-data-source.ts",
    "seed:run": "yarn typeorm:ts migration:run -d ./src/database/seed-data-source.ts",
    "seed-win:run": "yarn typeorm migration:run -d ./src/database/seed-data-source.ts",
    "data:init": "yarn typeorm:dropschema && yarn migration:run && yarn seed:run",
    "data-win:init": "yarn typeorm-win:dropschema && yarn migration-win:run && yarn seed-win:run",
    "data:reset": "yarn seed:revert && yarn migration:revert && yarn typeorm:dropschema && yarn data:init",
    "data:e2einit": "NODE_ENV=test yarn typeorm:dropschema && NODE_ENV=test yarn migration:run",
    "data-win:e2einit": "NODE_ENV=test yarn typeorm-win:dropschema && NODE_ENV=test yarn migration-win:run",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier --check .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "commit": "better-commits",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-presigned-post": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@faker-js/faker": "^8.3.1",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.3",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "9.4.3",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "9.4.1",
    "@sindresorhus/slugify": "^2.2.1",
    "@types/mime": "^3.0.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.0",
    "bull": "^4.12.2",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.9",
    "helmet": "^7.0.0",
    "isbot": "^3.7.1",
    "jsonwebtoken": "^9.0.0",
    "lodash-es": "4.17.20",
    "mime": "^3.0.0",
    "nestjs-pino": "^3.2.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "patch-package": "^7.0.2",
    "pg": "^8.11.3",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "postinstall-postinstall": "^2.1.0",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.6",
    "socket.io": "4.6.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.10",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "better-commits": "^1.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
